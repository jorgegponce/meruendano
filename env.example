# Copy this to `.env` and fill in your own keys/paths.

# Zotero Web API (or Group API) credentials
ZOTERO_LIBRARY_ID=    # e.g. 1234567
ZOTERO_LIBRARY_TYPE=user   # or "group"
ZOTERO_API_KEY=       # your private key from Zotero

# (Optional) If you use Better BibTeX JSON export instead of the API:
ZOTERO_JSON_PATH=     # e.g. ./vault/zotero.json

# Local Zotero storage (for PDF files & annotations)
ZOTERO_STORAGE_PATH=  # e.g. ~/Zotero/storage

# OpenAlex API key (if you plan to enrich via OpenAlex)
OPENALEX_API_KEY=     

